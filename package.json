{
  "name": "vue-composable-testing",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@9.11.0",
  "description": "Test composables in Vue.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "composition api",
    "composable",
    "testing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "tsdown": "^0.13.3",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "vue": "^3.5.18"
  }
}
